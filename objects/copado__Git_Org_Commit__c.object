<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>copado__Commit_Id_Status_Complete</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !ISBLANK(copado__Commit_Id__c), not(ISPICKVAL(copado__Status__c,&quot;Complete&quot;)))</errorConditionFormula>
        <errorDisplayField>copado__Status__c</errorDisplayField>
        <errorMessage>The commit status must be &apos;Complete&apos; whenever there is a commit Id.</errorMessage>
    </validationRules>
</CustomObject>
